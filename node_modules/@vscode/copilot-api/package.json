{
  "name": "@vscode/copilot-api",
  "description": "A module used for interacting with the GitHub Copilot API.",
  "version": "0.2.6",
  "type": "module",
  "author": {
    "name": "Microsoft Corporation"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "module-sync": "./dist/index.js",
    "import": "./dist/index.js",
    "require": "./dist/index.js",
    "default": "./dist/index.js"
  },
  "sideEffects": false,
  "license": "SEE LICENSE",
  "scripts": {
    "build": "node esbuild.config.js && npm run build:types",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist src/index.ts ",
    "build:dev": "node esbuild.config.js --dev",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "compile": "npm run build",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "build:scripts": "tsc --project scripts/tsconfig.json",
    "mixin": "npm run build:scripts && node dist/scripts/scripts/mixin.js"
  },
  "devDependencies": {
    "@types/node": "^22.13.4",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "esbuild": "^0.25.5",
    "eslint": "^9.28.0",
    "eslint-plugin-notice": "^1.0.0",
    "jsdom": "^26.1.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/vscode-extension-telemetry.git"
  },
  "bugs": {
    "url": "https://github.com/Microsoft/vscode-extension-telemetry/issues"
  }
}