    "repository": {
        "type": "git",
        "url": "https://github.com/OEvortex/better-copilot-chat"
    },
    "engines": {
        "vscode": ">=1.104.0",
        "node": ">=20.0.0",
        "npm": ">=9.0.0"
    },
    "extensionDependencies": [
        "github.copilot-chat"
    ],
    "categories": [
        "AI",
        "Chat"
    ],
    "icon": "./logo_ai.png",
    "main": "./dist/extension.js",
    "enabledApiProposals": [
        "chatProvider"
    ],
    "activationEvents": [
