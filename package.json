                },
                "chp.fimCompletion.enabled": {
                    "type": "boolean",
                    "default": false,
                    "description": "Enable FIM (Fill In the Middle) suggestion feature.",
                    "scope": "application"
                },
                "chp.fimCompletion.debounceMs": {
                    "type": "number",
                    "default": 500,
                    "minimum": 50,
                    "maximum": 1000,
                    "description": "Debounce delay for FIM completion (milliseconds).",
                    "scope": "application"
                },
                "chp.fimCompletion.timeoutMs": {
                    "type": "number",
                    "default": 5000,
                    "minimum": 1000,
                    "maximum": 30000,
                    "description": "Request timeout for FIM completion (milliseconds).",
                    "scope": "application"
                },
                "chp.fimCompletion.modelConfig": {
                    "type": "object",
                    "default": {
                        "provider": "",
                        "baseUrl": "",
                        "model": ""
                    },
                    "description": "Model configuration for FIM completion.",
                    "scope": "application",
                    "properties": {
                        "provider": {
                            "type": "string",
                            "description": "Model provider identifier"
                        },
                        "baseUrl": {
                            "type": "string",
                            "description": "API base URL",
                            "format": "uri"
                        },
                        "model": {
                            "type": "string",
                            "description": "Model name used for FIM completion"
                        },
                        "maxTokens": {
                            "type": "number",
                            "description": "Maximum length returned by FIM completion",
                            "default": 200,
                            "minimum": 50,
                            "maximum": 16000
                        },
                        "extraBody": {
                            "type": "object",
                            "description": "Additional request body parameters, merged into the request body",
                            "additionalProperties": {
                                "description": "Additional request body parameter values"
                            }
                        }
                    },
                    "required": [
                        "provider",
                        "baseUrl",
                        "model"
                    ]
                },
                "chp.nesCompletion.enabled": {
                    "type": "boolean",
                    "default": false,
                    "description": "Enable NES (Next Edit Suggestions) suggestion feature.",
                    "scope": "application"
                },
                "chp.nesCompletion.debounceMs": {
                    "type": "number",
                    "default": 500,
                    "minimum": 50,
                    "maximum": 1000,
                    "description": "Debounce delay for NES completion (milliseconds).",
                    "scope": "application"
                },
                "chp.nesCompletion.timeoutMs": {
                    "type": "number",
                    "default": 5000,
                    "minimum": 1000,
                    "maximum": 30000,
                    "description": "Request timeout for NES completion (milliseconds).",
                    "scope": "application"
                },
                "chp.nesCompletion.manualOnly": {
                    "type": "boolean",
                    "default": false,
                    "description": "When enabled, NES completion only responds to manual triggers (Alt+/), will not trigger automatically.",
                    "scope": "application"
                },
                "chp.nesCompletion.modelConfig": {
                    "type": "object",
                    "default": {
                        "provider": "",
                        "baseUrl": "",
                        "model": ""
                    },
                    "description": "Model configuration for NES completion.",
                    "scope": "application",
                    "properties": {
                        "provider": {
                            "type": "string",
                            "description": "Model provider identifier"
                        },
                        "baseUrl": {
                            "type": "string",
                            "description": "API base URL",
                            "format": "uri"
                        },
                        "model": {
                            "type": "string",
                            "description": "Model name used for NES completion"
                        },
                        "maxTokens": {
                            "type": "number",
                            "description": "Maximum length returned by NES completion",
                            "default": 200,
                            "minimum": 50,
                            "maximum": 16000
                        },
                        "extraBody": {
                            "type": "object",
                            "description": "Additional request body parameters, will be merged into request body in API requests",
                            "additionalProperties": {
                                "description": "Additional request body parameter value"
                            }
                        }
                    },
                    "required": [
                        "provider",
                        "baseUrl",
                        "model"
                    ]
                },
                "chp.providerOverrides": {
                    "type": "object",
                    "default": {},
                    "description": "Provider configuration override. Allows overriding provider baseUrl and model configuration, supports adding new models or overriding existing model parameters.",
                    "scope": "application"
                },
                "chp.compatibleModels": {
                    "type": "array",
                    "default": [],
                    "description": "Custom model configuration for Compatible Provider. Completely user-configured OpenAI/Anthropic compatible model list.",
                    "scope": "application"
                },
                "antigravityQuotaWatcher.apiKey": {
                    "type": "string",
                    "default": "",
                    "description": "API key for Antigravity quota watcher.",
                    "scope": "application"
                }
            }
        },
        "jsonValidation": [
            {
                "fileMatch": "settings.json",
                "url": "chp-settings://root/schema.json"
